#!groovy
//run docker build
properties([disableConcurrentBuilds()])

pipeline {
	agent {
	label 'master'
	}
	triggers { pollSCM('* * * * *') }
	stages {
		stage("create docker image") {
			steps {
			echo "---------starting building image---------"
			dir ('docker/toolbox') {
					sh 'docker build . '
			}
			}
		}
	}
}
